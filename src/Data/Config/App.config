<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<appSettings>
		<!--常规设置-->
			<!--相关路径-->


		<!--语言 & 字体-->
			<add key="language" value="en-US"/>		<!--语言-->
		
			<!--消息通知-->
			<add key="auto_off_time" value="2"/>		<!--自动关闭时间-->
		
			<!--其他-->
			<add key="boot" value="False"/>		<!--开机启动-->
			<add key="auto_check_update" value="True"/>		<!--自动检查更新-->
			<add key="exit_program_mode" value="1"/>		<!--退出程序模式-->
		
		<!--个性化设置-->
			<!--启动 & 界面-->
			<add key="apply_theme" value=""/>	<!--应用主题-->
			<add key="startup_pic_mode" value=""/>	<!--启动图片模式-->
			<add key="startup_pic" value=""/>	<!--启动图片(仅用户选择固定启动图)-->

		<!--绘图设置-->
			<add key="coreldraw_path" value=""/> <!--CorelDRAW 程序路径-->
			<add key="inkscape_path" value=""/> <!--Inkscape 程序路径-->
			<add key="auto_check_template_update" value="True"/> <!--是否自动检查模板更新-->
	</appSettings>
</configuration>